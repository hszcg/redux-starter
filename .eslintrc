{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-uses-react": 1,
    "react/prefer-stateless-function": 0,
    "comma-dangle": 0,
    "dot-notation": 1,
    "react/jsx-quotes": 0,
    "new-cap": 0,
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
    "import/no-unresolved": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "globals": {
    "React": true
  },
  "settings": {
    "import/resolver": {
      "webpack": { "config": "webpack.config.js" }
    }
  },
  "plugins": [
    "react"
  ]
}
